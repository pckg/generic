<!DOCTYPE html>
<html>
<head>
    {% include '_head.twig' %}

    <link rel="icon" type="image/png" href="/app/{{ config('app') }}/public/img/favicon.ico"/>
    <link rel="shortcut icon" href="/app/{{ config('app') }}/public/img/favicon.ico">
</head>
<body class="{{ _router.getCssName() }} app-{{ config('app') }}">

{% include '_body.twig' %}

<div id="vue-app" class="container">
    <div class="row">
        <div class="col-xs-12">
            {% block content %}{{ content | raw }}{% endblock %}
        </div>
    </div>
</div>

{% include '_foot.twig' %}

</body>
</html>